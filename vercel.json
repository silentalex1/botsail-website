{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/botsail",
      "dest": "/public/botsail.html"
    },
    {
      "src": "/myapps",
      "dest": "/public/myapps.html"
    },
    {
      "src": "/callback",
      "dest": "/public/callback.html"
    },
    {
      "src": "/mobile-installer",
      "dest": "/public/mobile-installer.html"
    },
    {
      "src": "/background visuals/(.*)",
      "dest": "/public/background visuals/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
} 